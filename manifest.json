{
  "manifest_version": 2,
  "name": "joomla! control",
  "author": "Felipe Derkian de Sousa Freitas",
  "version": "1.0.12",
  "content_scripts": [
    /* pagina de cadastro phoca download */
    {
      "matches": [
        "https://saude.rr.gov.br/administrator/index.php?option=com_phocadownload&view=phocadownloadfile&layout=edit"
      ],
      "css": [
        "./css/index.css"
      ],
      "js": [
        // "./scripts/phoca/inputs_display_none.js", //j√° esta sendo importado em baixo
        "./scripts/phoca/date_start_publish_controler.js",
        "./scripts/phoca/form_complete.js",
        // "./scripts/phoca/shortcuts.js", // ja esta sendo importado em baixo
        "./scripts/phoca/description_controller.js",
        "./scripts/phoca/publication_automatic.js"
      ]
    },
    /* pagina de editar phoca download */
    {
      "matches": [
        "https://saude.rr.gov.br/administrator/index.php?option=com_phocadownload&view=phocadownloadfile&layout=edit*"
      ],
      "js": [
        "./scripts/phoca/inputs_display_none.js",
        "./scripts/phoca/shortcuts.js"
      ]
    },
    /* pagina de listagem do phoca downloads */
    {
      "matches": [
        "https://saude.rr.gov.br/administrator/index.php?option=com_phocadownload&view=phocadownloadfiles"
      ],
      "js": [
        "./scripts/phoca/shortcut_phoca_listagem.js",
        "./scripts/select_listagem_phoca_by_id/select_listagem_by_id_desc.js"
      ]
    },
    /* pagina de cadastro de noticias */
    {
      "matches": [
        "https://saude.rr.gov.br/administrator/index.php?option=com_content&view=article&layout=edit"
      ],
      "js": [
        "./scripts/articles/form_control.js"
      ]
    }
    ,
    /* Shortcuts joomla phoca downloads e Cache */
    {
      "matches": [
        "https://saude.rr.gov.br/administrator/index.php*"
      ],
      "js": [
        "./scripts/joomla/joomla_shortcuts.js",
        "./scripts/joomla/add_btn_head_clear_cache.js",
        "./scripts/joomla/add_btn_head_phoca_download.js"
      ]
    }
    ,
    /* reload pagina sei */
    {
      "matches": [
        "https://sei.rr.gov.br/sei/*"
      ],
      "js": [
        "./scripts/sei/sei_controller.js",
        "./scripts/sei/sei_shortcuts.js"
      ]
    }
    ,
    /* pagina sei login */
    {
      "matches": [
        "https://sip.rr.gov.br/sip/login.php*"
      ],
      "js": [
        "./scripts/sei/login.styles.js"
      ]
    }
  ],
  "browser_action": {
    "default_icon": "./logo/logo.png"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]
}